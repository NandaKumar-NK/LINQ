      <Association Name="Train_detail_booking_deatil" Member="booking_deatils" ThisKey="train_id" OtherKey="train_id" Type="booking_deatil" />
    </Type>
  </Table>
  <Table Name="dbo.passanger_details" Member="passanger_details">
