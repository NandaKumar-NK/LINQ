<?xml version="1.0" encoding="utf-8"?><Database Name="Train_TicketBooking" Class="DataClasses1DataContext" xmlns="http://schemas.microsoft.com/linqtosql/dbml/2007">
  <Table Name="dbo.Train_details" Member="Train_details">
